{
  "rewrites": [
    {
      "source": "/",
      "destination": "/test/unit/index.html"
    },
    {
      "source": "/:path*.js",
      "destination": "/test/unit/:path*.mjs"
    },
    {
      "source": "/:path",
      "destination": "/test/unit/:path"
    }
  ],
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Resource-Policy",
          "value": "cross-origin"
        }
      ]
    }
  ]
}
